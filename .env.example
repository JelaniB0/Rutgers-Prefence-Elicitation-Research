# env file may grow in terms of variables, for example, when adding ai services/keys or server information or other config options.
# Modes: "azure_key" = shared connection string, "azure_login" = individual Azure login, "local" = offline
ENV=azure_login

# For shared connection string mode only
# AZURE_STORAGE_CONNECTION_STRING=DefaultEndpointsProtocol=https;AccountName=<ACCOUNT_NAME>;AccountKey=<ACCOUNT_KEY>;EndpointSuffix=core.windows.net -- use for solo use. 
AZURE_STORAGE_ACCOUNT_NAME=<ACCOUNT_NAME> # use for RBAC -- most likely for this research project. 

# For local fallback
LOCAL_DB_PATH=local.db